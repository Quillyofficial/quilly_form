{
    "version": 2,
    "builds": [
      {
        "src": "src/pages/api/send-to-airtable.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/pages/**/*.{js,ts,jsx,tsx}",
        "use": "@vercel/next",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/send-to-airtable",
        "dest": "src/pages/api/send-to-airtable.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src/pages/$1"
      }
    ]
  }
  