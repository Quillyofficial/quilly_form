{
    "version": 2,
    "builds": [
        {
            "src": "src/pages/api/send-to-airtable.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/index.html",
            "use": "@vercel/static"
          }
    ],
    "routes": [
        {
            "src": "/api/send-to-airtable",
            "dest": "src/pages/api/send-to-airtable.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ]
}
